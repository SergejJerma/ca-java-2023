<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Darbuotojas" table="darbuotojai">
        <id name="id" type="int">
            <column name="id" />
            <generator class ="native"/>
        </id>
        <property name="vardas" type="string">
            <column name="vardas"/>
        </property>
        <property name="pavarde" type="string">
            <column name="pavarde"/>
        </property>
        <property name="atlyginimas" type="int">
            <column name="atlyginimas"/>
        </property>
    </class>
</hibernate-mapping>
